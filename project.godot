; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Digital Art Final"
config/features=PackedStringArray("4.4", "Forward Plus")
config/icon="res://icon.svg"

[shader_globals]

Heightmap={
"type": "sampler2D",
"value": "res://Heightmap.tres"
}
HeightOffset={
"type": "float",
"value": -0.5
}
HeightScale={
"type": "float",
"value": 2.0
}
Densitymap={
"type": "sampler2D",
"value": "res://DensityMap.tres"
}
Wind={
"type": "sampler2D",
"value": "res://Wind.tres"
}
WindDir={
"type": "vec2",
"value": Vector2(-0.25, 0)
}
WindSpeed={
"type": "vec2",
"value": Vector2(0.1, 0)
}
